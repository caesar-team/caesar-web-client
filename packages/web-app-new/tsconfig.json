{
  "extends": "../../tsconfig.json",
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "compilerOptions": {
    "paths": {
      "@caesar/components/*": ["../components/*"],
      "@caesar/components": ["../components/index"],
      "@caesar/assets/*": ["../assets/*"],
      "@caesar/assets": ["../assets/index"],
      "@caesar/common/*": ["../common/*"],
      "@caesar/common": ["../common/index"],
      "@caesar/containers/*": ["../containers/*"],
      "@caesar/containers": ["../containers/index"]
    }
  }
}
